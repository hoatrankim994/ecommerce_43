<tr>
  <td><%= user.id %></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.gender %></td>
  <td><%= user.address %></td>
  <td><%= user.phone %></td>
  <td>
    <%= link_to admin_user_path(user), method: :delete,
      data: {confirm: t("admin.users.destroy.confirm")} do %>
      <i class="fa fa-trash-o"></i>
    <% end %>
  </td>
</tr>
